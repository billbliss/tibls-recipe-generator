    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.1",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.34.3",
    "uuid": "^9.0.1"
