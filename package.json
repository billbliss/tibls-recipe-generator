{
  "name": "tibls-webhook-handler",
  "type": "commonjs",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "lint": "eslint . --ext .ts,.js",
